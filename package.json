{
  "name": "koa-versioned",
  "version": "0.1.0",
  "author": "Andrew Ewing <andrew.ewing@colonystarwood.com>",
  "repository": "https://github.com/colonyamerican/koa-versioned",
  "main": "lib/versioned.js",
  "files": ["lib/versioned.js"],
  "license": "MIT",
  "scripts": {
    "build": "node_modules/.bin/babel --minified --no-comments src/versioned.js -o lib/versioned.js",
    "watch": "node_modules/.bin/babel --watch --minified --no-comments src/versioned.js -o lib/versioned.js"
  },
  "babel": {
    "presets": ["es2015", "stage-0", "flow"],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "flow-bin": "^0.52.0"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0"
  }
}
